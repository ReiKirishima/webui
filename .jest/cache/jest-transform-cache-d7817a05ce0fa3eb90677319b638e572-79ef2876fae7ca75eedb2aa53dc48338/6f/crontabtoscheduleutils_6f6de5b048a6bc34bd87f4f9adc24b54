"use strict";
Object.defineProperty(exports, "__esModule", { value: true });
exports.crontabToSchedule = crontabToSchedule;
exports.crontabToScheduleWithoutMinutes = crontabToScheduleWithoutMinutes;
function crontabToSchedule(crontab) {
    const [minute, hour, dom, month, dow] = crontab.split(' ');
    return {
        minute,
        hour,
        dom,
        month,
        dow,
    };
}
function crontabToScheduleWithoutMinutes(crontab) {
    const [, hour, dom, month, dow] = crontab.split(' ');
    return {
        hour,
        dom,
        month,
        dow,
    };
}
